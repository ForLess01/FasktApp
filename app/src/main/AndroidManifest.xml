<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FasktApp"
        tools:targetApi="31">
        <activity
            android:name=".TimeLine_navbar_MainActivity"
            android:exported="false"
            android:label="@string/title_activity_time_line_navbar_main" />
        <activity
            android:name=".TimeLine_Main_Activity"
            android:exported="false" />
        <activity
            android:name=".RegisterOrLogInON_Activity"
            android:exported="false" />
        <activity
            android:name=".WelcomeOFF_Activity"
            android:exported="false" />
        <activity
            android:name=".Register_OFF_Activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>